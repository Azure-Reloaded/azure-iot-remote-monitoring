<?xml version="1.0" encoding="utf-8"?>

<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:jws="http://api.jasperwireless.com/ws/schema"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://api.jasperwireless.com/ws/schema"
             xmlns="http://schemas.xmlsoap.org/wsdl/">
  <types>
    <xs:schema xmlns="http://api.jasperwireless.com/ws/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               attributeFormDefault="unqualified" elementFormDefault="qualified"
               xsi:schemaLocation="http://www.w3.org/2001/XMLSchema http://www.w3.org/2001/XMLSchema.xsd"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:import schemaLocation="JasperAPI.xsd" namespace="http://api.jasperwireless.com/ws/schema" />
    </xs:schema>
  </types>
  <message name="EchoRequest">
    <part name="body" element="jws:EchoRequest" />
  </message>
  <message name="EchoResponse">
    <part name="body" element="jws:EchoResponse" />
  </message>
  <portType name="EchoPortType">
    <operation name="Echo">
      <input message="jws:EchoRequest" />
      <output message="jws:EchoResponse" />
    </operation>
  </portType>
  <binding name="EchoBinding" type="jws:EchoPortType">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http" />
    <operation name="Echo">
      <soap:operation soapAction="http://api.jasperwireless.com/ws/service/echo/Echo" />
      <input name="EchoRequest">
        <soap:body use="literal" namespace="http://api.jasperwireless.com/ws/schema" />
      </input>
      <output name="EchoResponse">
        <soap:body use="literal" namespace="http://api.jasperwireless.com/ws/schema" />
      </output>
    </operation>
  </binding>
  <service name="EchoService">
    <port name="EchoPort" binding="jws:EchoBinding">
      <soap:address location="https://api7.jasperwireless.com/ws/service/echo" />
    </port>
  </service>
</definitions>